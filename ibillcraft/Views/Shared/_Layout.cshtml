		// If container-fluid exists, remove bundle.style.css and switch to style.css
		if (containerFluidExists) {
			var bundleStyleSheet = document.getElementById('styleSheetLink');
			bundleStyleSheet.parentNode.removeChild(bundleStyleSheet);

			var styleCssLink = document.createElement('link');
			styleCssLink.rel = 'stylesheet';
			styleCssLink.href = '~/css/site.css';
			document.head.appendChild(styleCssLink);
		}
		$(window).on('unload', function () {
			setTimeout(function () {
				$(window).scrollTop(0);
