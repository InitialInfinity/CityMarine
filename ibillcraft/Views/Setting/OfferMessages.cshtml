        <div class="modal fade" id="addSubscription" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
             aria-hidden="true">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title text-dark fw-bold" id="exampleModalLabel">Add Country Master</h5>
                        <button type="button"
                                class="btn-close modal-close"
                                data-bs-dismiss="modal"
                                aria-label="Close"></button>
                    </div>
                    <div class="modal-body text-dark">
                        <div class="row">
                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                                <div class="form-group form-group-sm">
                                    <label class="control-label fw-bold">
                                        Country Name <span class="text-danger">*</span>
                                    </label>
                                    <input type="text"
                                           id="countryName"
                                           name="countryName"
                                           class="form-control mt-2"
                                           autoComplete="off"
                                           placeholder="Enter Country Name"
                                           required />
                                </div>
                            </div>

                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                                <div class="form-group form-group-sm">
                                    <label class="control-label fw-bold">
                                        Country Code <span class="text-danger">*</span>
                                    </label>
                                    <input type="text"
                                           id="countryCode"
                                           name="countryCode"
                                           autoComplete="off"
                                           class="form-control mt-2"
                                           placeholder="Enter Country Code"
                                           required />
                                    <span id="message"></span>
                                </div>
                            </div>
                        </div>
                        <div class="row mt-2">
                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                                <div class="form-group form-group-sm">
                                    <label class="control-label fw-bold">
                                        Currency Name<span class="text-danger">*</span>
                                    </label>
                                    <select class="form-select mt-2">
                                        <option>1</option>
                                        <option>2</option>
                                        <option>3</option>
                                        <option>4</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                                <div class="form-group form-group-sm">
                                    <label class="control-label fw-bold">
                                        Timezone<span class="text-danger">*</span>
                                    </label>
                                    <input type="text"
                                           id="discount"
                                           name="discount"
                                           autoComplete="off"
                                           class="form-control mt-2"
                                           placeholder="Enter Timezone"
                                           required />
                                    <span id="message"></span>
                                </div>
                            </div>
                        </div>


                        <div class="row mt-2">

                            <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6 ">
                                @*                            <label class="control-label fw-bold">

                                </label>*@
