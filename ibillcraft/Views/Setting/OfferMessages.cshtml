@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = " Setting ";
    ViewBag.Header = "Setting";

}

<div class="card mb-4 border  profile-card">
    <h2 class="m-0 font-weight-bold text-primary">Offer Messages</h2>
    <div class="card-body">
        <form>
            <div class="row py-2">
                <div class="col-lg-4">
                    <label for="message" class="form-label fw-bold">Message<span class="text-danger">*</span></label>
                </div>
                <div class="col-lg-8">
                    <textarea rows="4" cols="50" class="form-control" id="name" placeholder="Enter Message" required></textarea>
                </div>
            </div>
            <div class="row py-2">
                <div class="card-footer d-flex justify-content-end">
                    <button type="button" class="btn btn-success mx-3">Send</button>
                    <button type="button" class="btn btn-primary mx-3">Cancel </button>
                </div>
            </div>
        </form>
    </div>
</div>

